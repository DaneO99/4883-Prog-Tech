#include <iostream>
#include <cmath> // For llabs() to handle absolute value of long long

using namespace std;

int main()
{
    long long num1, num2; // Use long long to handle large integers
    while (cin >> num1 >> num2)
    {
        // Calculate absolute difference using llabs for long long
        long long answer = llabs(num2 - num1);
        cout << answer << endl;
    }
    return 0;
}
